<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="contract_document_tag_model_action" model="ir.actions.act_window">
            <field name="name">Contract tag</field>
            <field name="res_model">contract.document.tag</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="contract_document_tag_model_view_tree" model="ir.ui.view">
            <field name="name">contract.document.tag.tree</field>
            <field name="model">contract.document.tag</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" string="Name"/>
                </tree>
            </field>
        </record>

        <record id="contract_document_tag_model_view_form" model="ir.ui.view">
            <field name="name">contract.document.tag.form</field>
            <field name="model">contract.document.tag</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="name" string="Name" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    
    </data>
    

</odoo>
